{
  "name": "Medium right to left support",
  "version": "0.3",
  "minimum_chrome_version": "49.0",
  "description": "Set RTL support for Medium articles",
  "icons": { "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png" },
  "content_scripts": [
    {
      "matches": [
        "https://medium.com/*"
      ],
      "js": [
        "content-script.js"
      ]
    }
  ],
  "manifest_version": 2
}
